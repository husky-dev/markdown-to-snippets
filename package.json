{
  "name": "md2snipp",
  "description": "Tool for generating VSCode snippets from markdown files",
  "version": "1.0.0",
  "keywords": [
    "markdown",
    "snippets",
    "vscode",
    "cli"
  ],
  "author": "Jaroslav Khorishchenko <jaro@smartapp.dev>",
  "repository": {
    "type": "git",
    "url": "https://github.com/husky-dev/md2snipp.git"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "bin": {
    "md2snipp": "./dist/index.js"
  },
  "files": [
    "README.md",
    "package.json",
    "LICENSE",
    "dist"
  ],
  "scripts": {
    "start": "node ./dist/index.js",
    "dist": "yarn dist:dev",
    "dist:dev": "webpack --mode=development",
    "dist:prd": "webpack --mode=production",
    "link": "yarn global add file:$PWD",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint:types": "tsc --noEmit --skipLibCheck",
    "lint:eslint": "eslint \"src/**\"",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "clear": "rm -rf .cache dist",
    "prepare": "husky install"
  },
  "lint-staged": {
    "{*.ts,*tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "26.0.22",
    "@types/node": "14.14.37",
    "@typescript-eslint/eslint-plugin": "4.20.0",
    "@typescript-eslint/parser": "4.20.0",
    "commander": "7.2.0",
    "eslint": "7.23.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "ts-jest": "26.5.4",
    "ts-loader": "8.1.0",
    "tsconfig-paths": "3.9.0",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "typescript": "4.2.3",
    "webpack": "5.28.0",
    "webpack-cli": "4.6.0"
  }
}
